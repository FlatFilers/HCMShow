# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

NEXTAUTH_SECRET="some-secret"
# Note: this has to be saved as a secret in Flatfile's dashboard
SERVER_AUTH_TOKEN="boopboop"
BASEPATH_URL="http://localhost:3000"
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/hcm_show_development?schema=public"

# Onboarding workflow
ONBOARDING_FLATFILE_API_KEY=sk_...
ONBOARDING_ENVIRONMENT_ID=us_env_...
ONBOARDING_WORKBOOK_NAME="HCM Workbook"

# Filefeed workflow
FILEFEED_API_KEY=sk_...
FILEFEED_ENVIRONMENT_ID=us_env_...

# Embedded workflow
NEXT_PUBLIC_EMBEDDED_PUBLISHABLE_KEY=pk_...
EMBEDDED_FLATFILE_API_KEY=sk_...
EMBEDDED_ENVIRONMENT_ID=us_env_...
EMBEDDED_WORKBOOK_NAME="Benefits Workbook"

# Dynamic workflow
NEXT_PUBLIC_DYNAMIC_PUBLISHABLE_KEY=pk_...
DYNAMIC_FLATFILE_API_KEY=sk_...
DYNAMIC_TEMPLATES_ENVIRONMENT_ID=us_env_...
DYNAMIC_TEMPLATES_WORKBOOK_NAME="Benefits Workbook"

